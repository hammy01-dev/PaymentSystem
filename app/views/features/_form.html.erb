
<%= form_with model:feature  do |form|%>
  <div class="field form-group">
    <%= form.label :name %><br>

    <%= form.text_field :name,required: true %>
  </div>
  <div class="field form-group ">
    <%= form.label :code %><br>
    <%= form.text_field :code , required: true%>

  </div>
  <div class="field form-group ">
    <%= form.label :unit_price %><br>
    <%= form.text_field :unit_price %>
  </div>

  <div>
    <%= form.label :max_unit_limit %><br>
    <%= form.text_area :max_unit_limit %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
